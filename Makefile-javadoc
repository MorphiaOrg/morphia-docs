~/.m2/repository/dev/morphia/morphia/morphia-core/3.0.0-SNAPSHOT/morphia-core-3.0.0-SNAPSHOT-javadoc.jar:
	@mvn dependency:get -DgroupId=dev.morphia.morphia -DartifactId=morphia-core -Dversion=3.0.0-SNAPSHOT \
		-Dclassifier=javadoc -DremoteRepositories=https://oss.sonatype.org/content/repositories/snapshots/ -Dtransitive=false

build/site/morphia/3.0/javadoc/index.html: ~/.m2/repository/dev/morphia/morphia/morphia-core/3.0.0-SNAPSHOT/morphia-core-3.0.0-SNAPSHOT-javadoc.jar
	@mkdir -p build/site/morphia/3.0/javadoc/
	@unzip -DD -q -o ~/.m2/repository/dev/morphia/morphia/morphia-core/3.0.0-SNAPSHOT/morphia-core-3.0.0-SNAPSHOT-javadoc.jar \
		-d build/site/morphia/3.0/javadoc/ ;

