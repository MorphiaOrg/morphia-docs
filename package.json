{
  "description": "Morphia docs",
  "scripts": {
    "clean-build": "npm run clean-install;npm run build",
    "clean-local-build": "npm run clean-install;npm run local-build",
    "clean": "rm -rf node_modules/ .cache/ package-lock.json",
    "clean-install": "npm run clean;npm i --cache=.cache/npm",
    "build": "antora antora-playbook.yml --stacktrace --fetch",
    "local-build": "antora local-antora-playbook.yml --stacktrace --fetch"
  },
  "devDependencies": {
    "@antora/cli": "3.0.3",
    "@antora/site-generator": "3.0.3",
    "@djencks/asciidoctor-openblock": "^0.0.1",
    "@djencks/playbook-builder": "https://experimental-repo.s3-us-west-1.amazonaws.com/djencks-playbook-builder-v2.3.3.tgz"
  },
  "dependencies": {
    "npm": "^8.3.0"
  }
}
